{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "runtimeArgs": [
          "--inspect"
        ],
        "args": [
          "${workspaceFolder}/src/main.ts"
        ],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "sourceMaps": true
      }
    ]
  }
  